{
  "name": "khaneliman",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "build": "ng build",
    "build-prod": "ng build --aot --common-chunk --build-optimizer",
    "build:ssr": "ng run WebApp:server:dev",
    "test": "ng test",
    "test:e2e": "ng e2e austin-horstman-e2e --record --key 86eb1ae2-fcd7-480d-ba50-986beb7097e6",
    "test-coverage": "ng test --code-coverage",
    "lint": "eslint apps/austin-horstman",
    "lint:html": "eslint apps/austin-horstman -f html -o coverage/apps/austin-horstman/eslint.html",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts apps/austin-horstman",
    "prettier-format": "prettier --config .prettierrc apps/austin-horstman/**/*.{ts,js,html} --write",
    "clean:node": "rm -rf node_modules/",
    "clean:dist": "rm -rf dist/",
    "i": "npm install",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44463 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44463 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "14.2.7",
    "@angular/cdk": "14.2.5",
    "@angular/common": "14.2.7",
    "@angular/compiler": "14.2.7",
    "@angular/core": "14.2.7",
    "@angular/forms": "14.2.7",
    "@angular/localize": "14.2.7",
    "@angular/material": "14.2.5",
    "@angular/platform-browser": "14.2.7",
    "@angular/platform-browser-dynamic": "14.2.7",
    "@angular/platform-server": "14.2.7",
    "@angular/router": "14.2.7",
    "@ng-bootstrap/ng-bootstrap": "^13.1.0",
    "@ngrx/store": "14.0.2",
    "@nrwl/angular": "15.9.7",
    "@popperjs/core": "^2.10.2",
    "ng2-nouislider": "^1.8.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.12",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/cli": "14.2.12",
    "@angular/compiler-cli": "14.2.7",
    "@angular/language-service": "14.2.7",
    "@nrwl/cli": "15.0.1",
    "@nrwl/cypress": "15.0.1",
    "@nrwl/eslint-plugin-nx": "15.9.7",
    "@nrwl/jest": "15.0.1",
    "@nrwl/linter": "15.0.1",
    "@nrwl/nx-cloud": "18.0.0",
    "@nrwl/tao": "15.9.7",
    "@nrwl/workspace": "15.0.1",
    "@schematics/angular": "14.2.6",
    "@types/jest": "28.1.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "chromedriver": "^119.0.1",
    "cypress": "^13.7.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "28.1.3",
    "jest-preset-angular": "12.2.2",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.31",
    "prettier": "2.7.1",
    "puppeteer": "^19.1.0",
    "run-script-os": "^1.1.6",
    "stylelint": "^14.7.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-jest": "28.0.8",
    "typescript": "4.8.4"
  }
